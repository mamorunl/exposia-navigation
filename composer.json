{
  "name": "rapideinternet/exposia-navigation",
  "description": "Exposias Navigation package.",
  "keywords": [
    "framework",
    "laravel",
    "exposia"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "symfony/dom-crawler": "3.0.*@dev",
    "symfony/css-selector": "3.0.*@dev",
    "illuminate/html": "5.*",
    "rapideinternet/exposia": "dev-master",
    "intervention/image": "dev-master",
    "rapideinternet/laravel-searchable": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rapideinternet/exposia"
    },
    {
      "type": "vcs",
      "url":  "git@github.com:rapideinternet/laravel-searchable.git"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "benconstable/phpspec-laravel": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Exposia\\Navigation\\": "src/"
    }
  }
}
